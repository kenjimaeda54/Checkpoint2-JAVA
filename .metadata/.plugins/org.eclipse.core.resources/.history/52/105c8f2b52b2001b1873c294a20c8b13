package checkpoint2;

public class Convert {
	double temperature;
	double  results;
	
	
	
	public double getTemperature() {
		return temperature;
	}



	public void setTemperature(double temperature) {
		this.temperature = temperature;
	}



	private double celsius(double temperature) {
	    this.results =  this.temperature * 1.8 + 32;
	    return results;
		
	}
	private double fahrenheit(double temperature) {
	    this.results =  (this.temperature -32) * 5/9;
	    return results;
		
	}
	
	
	
	

}
