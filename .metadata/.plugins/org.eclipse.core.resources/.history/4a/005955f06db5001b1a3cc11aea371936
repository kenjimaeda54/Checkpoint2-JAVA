package checkpoint2;

public class Summation {
	 private int number;
     private int initial;
     private int results;

	 
	private int getNumber() {
		return number;
	}

	protected void setNumber(int number) {
		this.number = number;
	}
	 
	public void   colletions() {
		for(initial=1;initial < number; initial++) {
			this.results += number;
			System.out.println(this.results);
		   
		}
	    System.out.println("Valor da soma de todos os numeros entre 1 ate seu numero Ã© "+this.results);
		
	}
	
	

}
