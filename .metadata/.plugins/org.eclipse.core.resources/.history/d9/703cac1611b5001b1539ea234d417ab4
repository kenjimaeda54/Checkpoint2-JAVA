package checkpoint2;

public class Summation {
	 private int number;
     private int initial;
     private int results; 
	 
	private int getNumber() {
		return number;
	}

	protected void setNumber(int number) {
		this.number = number;
	}
	 
	protected void   colletions() {
		for(initial=1;initial < number; initial++) {
			this.results += number;
		    System.out.println("->>>"+results);
			
		}
	}

}
