package checkpoint2;

import java.lang.Math; 

public class Geometric {
	        double radius;
	        double field;
	        double basis;
	        double heigth;
	 
	        
	        
			public double getRadius() {
				return radius;
			}
			
			public void setRadius(double radius) {
				this.radius = radius;
			}
			
		
			public double getHeigth() {
				return heigth;
			}
			
			public void setHeigth(double heigth) {
				this.heigth = heigth;
			}
			
			public double getBasis() {
				return basis;
			}
			
			public void setBasis(double basis) {
				this.basis = basis;
			}
		     
			
	        
	        
	        protected  void   field(double radius){
	        	 this.field = (radius*radius)*Math.PI; 
	        	 System.out.println("Sua area Ã©"+this.field);
	        	
	        }
		    
	        protected  void rectangle(double basis,double height) {
	            this.field =  basis*height;     
	        }
		
	            

}
